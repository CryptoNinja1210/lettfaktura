{
  "version": 2,
  "builds": [
    {
      "src": "lettfaktura/wsgi.py",
      "use": "@vercel/python",
      "config": { "pythonVersion": "3.11.2" }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "lettfaktura/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_VERSION": "5.0"
  }
}
